<%- include("partials/header") -%>

    <h2>
        <%= composePageHeading %>
    </h2>

    <form action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input type="text" class="form-control" name="postTitle" placeholder="Title">
            <label>Post</label>
            <textarea class="form-control " rows="5 " name="postBody" placeholder="Your Message Here "></textarea>
        </div>
        <button type="submit " name="button " class="btn btn-primary pt-2 ">Publish</button>

    </form>

    <%- include("partials/footer") -%>